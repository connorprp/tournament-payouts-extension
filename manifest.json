{
  "manifest_version": 3,
  "name": "Payouts",
  "version": "1.0",
  "description": "Calculates payouts for golf tournaments",
  "action": {
    "default_popup": "payouts.html"
  },
  "icons": {
    "16": "/icons/golfball.png",
    "48": "/icons/golfball.png",
    "128": "/icons/golfball.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}

  